{
  "name": "mobile-pdf-upload-sign-view",
  "version": "1.0.0",
  "private": true,
  "description": "Mobile-friendly PDF upload, mock sign, and view app (React + Webpack + Express)",
  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "server": "ts-node --transpile-only server/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.21.1",
    "express-rate-limit": "7.4.0",
    "helmet": "7.1.0",
    "multer": "1.4.5-lts.1",
    "pdf-lib": "1.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.26.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/multer": "1.4.12",
    "@types/node": "22.7.7",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-loader": "9.2.1",
    "css-loader": "7.1.2",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.6.3",
    "style-loader": "4.0.0",
    "ts-node": "10.9.2",
    "typescript": "~5.5.4",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
